{
    "name": "gis-vt",
    "version": "1.0.0",
    "description": "modified version of geojson-vt",
    "keywords": [
        "vector-tile",
        "tiles",
        "geojson"
    ],
    "type": "module",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs.js"
        }
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dengcheke/gis-vt.git"
    },
    "scripts": {
        "dev": "vite --config ./vite.example.ts",
        "build-example": "vite build --config ./vite.example.ts",
        "build": "vite build --config ./vite.build.ts && npm run build-dts",
        "build-dts": "tsc -p tsconfig.build.json"
    },
    "devDependencies": {
        "@types/d3": "^7.4.3",
        "@types/earcut": "^3.0.0",
        "@types/node": "^24.3.0",
        "d3": "^7.9.0",
        "earcut": "^3.0.2",
        "es-toolkit": "^1.39.9",
        "fast-glob": "^3.3.3",
        "gl-matrix": "^3.4.4",
        "lil-gui": "^0.20.0",
        "proj4": "^2.19.10",
        "sass-embedded": "^1.90.0",
        "twgl.js": "^7.0.0",
        "typescript": "~5.8.3",
        "vite": "^7.0.4"
    },
    "dependencies": {
        "@types/geojson": "^7946.0.16"
    }
}